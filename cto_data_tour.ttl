@prefix cto: <http://data.cyclingtour.fr/data#> .
@prefix cs:  <http://data.cyclingtour.fr/schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

cto:Stage1 a cs:TourStage ;
    rdfs:label "Stage 1: Nice to Cannes" ;
    cs:stageNumber 1 ;
    cs:stagePath cto:Nice_Cannes_Path ;
    cs:capacity 12 ;
    cs:guideAssigned cto:Guide_Pierre .

cto:Stage2 a cs:TourStage ;
    rdfs:label "Stage 2: Cannes to Antibes" ;
    cs:stageNumber 2 ;
    cs:stagePath cto:Cannes_Antibes_Path ;
    cs:capacity 12 ;
    cs:guideAssigned cto:Guide_Pierre .

cto:Stage3 a cs:TourStage ;
    rdfs:label "Stage 3: Antibes to Grasse" ;
    cs:stageNumber 3 ;
    cs:stagePath cto:Antibes_Grasse_Path ;
    cs:capacity 10 ;
    cs:guideAssigned cto:Guide_Marie .

cto:Stage4 a cs:TourStage ;
    rdfs:label "Stage 4: Tomblaine to La Super Planche des Belles Filles" ;
    cs:stageNumber 4 ;
    cs:stagePath cto:Tomblaine_SuperPlanche_Path ;
    cs:capacity 8 ;
    cs:guideAssigned cto:Guide_Lucas .

cto:Stage5 a cs:TourStage ;
    rdfs:label "Stage 5: Lille to Arenberg_Porte_du_Hainaut" ;
    cs:stageNumber 5 ;
    cs:stagePath cto:Lille_ArenbergPorteduHainaut_Path ;
    cs:capacity 10 ;
    cs:guideAssigned cto:Guide_Lucas .

cto:Stage6 a cs:TourStage ;
    rdfs:label "Stage 6: Dunkerque to Calais" ;
    cs:stageNumber 6 ;
    cs:stagePath cto:Dunkerque_Calais_Path ;
    cs:capacity 10 ;
    cs:guideAssigned cto:Guide_Sophie .

cto:Stage7 a cs:TourStage ;
    rdfs:label "Stage 7: Châtel to Aigle" ;
    cs:stageNumber 7 ;
    cs:stagePath cto:Chatel_Aigle_Path ;
    cs:capacity 8 ;
    cs:guideAssigned cto:Guide_Marie .

