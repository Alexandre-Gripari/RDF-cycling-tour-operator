cto:CoastalEasyWeek a cs:TourPackage ;
    rdfs:label "Coastal Easy Week" ;
    cs:pricePerDay "125.00"^^xsd:decimal ;
    cs:capacity 12 ;
    cs:includesStage cto:Stage1, cto:Stage2, cto:Stage3 ;
    cs:guideAssigned cto:Guide_Pierre .

cto:MountainAdventureWeek a cs:TourPackage ;
    rdfs:label "Mountain Adventure Week" ;
    cs:pricePerDay "150.00"^^xsd:decimal ;
    cs:capacity 10 ;
    cs:includesStage cto:Stage4, cto:Stage5, cto:Stage6 ;
    cs:guideAssigned cto:Guide_Lucas .

cto:AlpsChallengeWeek a cs:TourPackage ;
    rdfs:label "Alps Challenge Week" ;
    cs:pricePerDay "175.00"^^xsd:decimal ;
    cs:capacity 8 ;
