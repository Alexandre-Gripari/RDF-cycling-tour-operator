    rdfs:label "Stage 8: Albertville to Col du Granon" ;
    cs:stageNumber 8 ;
    cs:stagePath cto:Albertville_ColduGranon_Path ;
    cs:capacity 6 ;
    cs:guideAssigned cto:Guide_Sophie .

cto:Stage9 a cs:TourStage ;
    rdfs:label "Stage 9: Granon to Briançon" ;
    cs:stageNumber 9 ;
    cs:stagePath cto:Granon_Briancon_Path ;
    cs:capacity 6 ;
    cs:guideAssigned cto:Guide_Marie .

